<?xml version="1.0" ?>
<robot name="PiDog">
    <webots>
        <!-- Define all motors - webots_ros2_driver auto-creates ROS interfaces -->
        <device reference="body_to_front_left_leg_b" type="RotationalMotor"/>
        <device reference="front_left_leg_b_to_a" type="RotationalMotor"/>
        <device reference="body_to_front_right_leg_b" type="RotationalMotor"/>
        <device reference="front_right_leg_b_to_a" type="RotationalMotor"/>
        <device reference="body_to_back_left_leg_b" type="RotationalMotor"/>
        <device reference="back_left_leg_b_to_a" type="RotationalMotor"/>
        <device reference="body_to_back_right_leg_b" type="RotationalMotor"/>
        <device reference="back_right_leg_b_to_a" type="RotationalMotor"/>
        <device reference="motor_8_to_tail" type="RotationalMotor"/>
        <device reference="neck1_to_motor_9" type="RotationalMotor"/>
        <device reference="neck2_to_motor_10" type="RotationalMotor"/>
        <device reference="neck3_to_motor_11" type="RotationalMotor"/>
    </webots>
</robot>
